<div class='demo-container fullscreen'>
  <div class="demo-banner">
    <h1>frost-bunsen-form</h1>
    <p>Component for rendering forms</p>
  </div>
  <div class="demo-showcase">
    {{frost-demo-editor path='component/form'}}
    <div class="demo-view">
      <br>
      {{frost-bunsen-form
        bunsenModel=formModel
        onChange=(action "onFormValueChange")
        onValidation=(action "onFormValidation")
        value=formValue
      }}
    </div>
  </div>
</div>
